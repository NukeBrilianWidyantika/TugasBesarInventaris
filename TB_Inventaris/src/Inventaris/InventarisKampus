        CCTV_ruang.setVisible(false);
        lingkungan_ruang.setVisible(false);
        kebersihan_ruang.setVisible(false);
        kenyamanan_ruang.setVisible(false);
        data.setVisible(false);
    }
    public void slider1(){
        txtnilaipencahayaan.setEditable(false);
        txtnilaipencahayaan.setText(null);
        s_nilaipencahayaan.setMinorTickSpacing(5);
        s_nilaipencahayaan.setMajorTickSpacing(20);
        s_nilaipencahayaan.setPaintTicks(true);
        s_nilaipencahayaan.setLabelTable(s_nilaipencahayaan.getLabelTable());
        s_nilaipencahayaan.setPaintLabels(true);
        s_nilaipencahayaan.addChangeListener(
                new ChangeListener() {
                    public void stateChanged (ChangeEvent e){
                        txtnilaipencahayaan.setText(String.valueOf(s_nilaipencahayaan.getValue()));
                }
            }
        );
    }
    public void slider2(){
        txtkelembapan.setEditable(false);
        txtkelembapan.setText(null);
