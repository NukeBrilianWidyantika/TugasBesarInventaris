    private void btnnextActionPerformed(java.awt.event.ActionEvent evt) {                                        
        if (identitas_ruang.isShowing() && !txtnama.getText().equalsIgnoreCase("") && !txtlokasi.getText().equalsIgnoreCase("") && !cmbfakultas.getSelectedItem().equals("Pilih Fakultas")) {
            previous_next();
            kondisi_ruang.setVisible(true);
            btnprevious.setEnabled(true);
        }else if(kondisi_ruang.isShowing() && !txtpanjang.getText().equalsIgnoreCase("") && !txtlebar.getText().equalsIgnoreCase("") && !txtjmlkursi.getText().equalsIgnoreCase("") && !txtjmlpintu.getText().equalsIgnoreCase("") && !txtjmljendela.getText().equalsIgnoreCase("")) {
            previous_next();
            sarana_ruang.setVisible(true);
            steker_ruang.setVisible(true);
        }else if(sarana_ruang.isShowing() && steker_ruang.isShowing()){
            previous_next();
            sarana_ruang.setVisible(true);
            kabel_LCD.setVisible(true);
        }else if(sarana_ruang.isShowing() && kabel_LCD.isShowing()){
            previous_next();
            sarana_ruang.setVisible(true);
            lampu_ruang.setVisible(true);
        }else if(sarana_ruang.isShowing() && lampu_ruang.isShowing()){
            previous_next();
            sarana_ruang.setVisible(true);
            kipas_ruang.setVisible(true);
        }else if(sarana_ruang.isShowing() && kipas_ruang.isShowing()){
            previous_next();
            sarana_ruang.setVisible(true);
            AC_ruang.setVisible(true);
        }else if(sarana_ruang.isShowing() && AC_ruang.isShowing()){
            previous_next();
            sarana_ruang.setVisible(true);
            CCTV_ruang.setVisible(true);
        }else if(sarana_ruang.isShowing() && CCTV_ruang.isShowing()){
            previous_next();
            sarana_ruang.setVisible(true);
            SSID.setVisible(true);
        }else if(sarana_ruang.isShowing() && SSID.isShowing()){
            previous_next();
            lingkungan_ruang.setVisible(true);
        }else if(lingkungan_ruang.isShowing()){
            previous_next();
            kebersihan_ruang.setVisible(true);
        }else if(kebersihan_ruang.isShowing()){
            previous_next();
            kenyamanan_ruang.setVisible(true);
        }else if(kenyamanan_ruang.isShowing()){
            previous_next();
            keamanan_ruang.setVisible(true);
            btnnext.setEnabled(false);
        }
        else{
            JOptionPane.showMessageDialog(null, "Data Belum Lengkap!!");
        }
        
    }                                       

    private void btnnewActionPerformed(java.awt.event.ActionEvent evt) {                                       
        //TODO add your handling code here:
        btnnext.setEnabled(true);
        btnprevious.setEnabled(false);
        
        if (btnnew.getText().equals("Baru")) {
            previous_next();
            identitas_ruang.setVisible(true);
            txtnama.setText("");
            txtlokasi.setText("");
            cmbfakultas.setSelectedItem("Pilih Fakultas");
            txtnama.setEnabled(true);
            txtlokasi.setEnabled(true);
            cmbfakultas.setEnabled(true);
            txtnama.requestFocus();
            btnnew.setText("Simpan");
            btnreset.setEnabled(true);
        } else {
            if (!txtnama.getText().equals("") && !txtlokasi.getText().equals("") && !cmbfakultas.getSelectedItem().equals("Pilih Fakultas") && !txtpanjang.getText().equals("") && !txtlebar.getText().equals("") && !txtjmlkursi.getText().equals("") && !txtjmlpintu.getText().equals("") && !txtjmljendela.getText().equals("")) {
                Koneksi konek = new Koneksi(); //buka koneksi 
                Model m = new Model();
                Model nk;
                Model call1 = new Model();
                m.setNama_ruang(txtnama.getText());
                m.setLokasi_ruang(txtlokasi.getText());
                m.setFakultas((String) cmbfakultas.getSelectedItem());
                m.setPanjang_ruang(Double.parseDouble(txtpanjang.getText()));
                m.setLebar_ruang(Double.parseDouble(txtlebar.getText()));
                m.setJml_kursi(Integer.parseInt(txtjmlkursi.getText()));
                m.setJml_pintu(Integer.parseInt(txtjmlpintu.getText()));
                m.setJml_jendela(Integer.parseInt(txtjmljendela.getText()));
                m.setJml_steker(Integer.parseInt(txtjmlsteker.getText()));
                m.setJml_baik_steker(Integer.parseInt(txtjmlsteker.getText()));
                m.setPosisi_steker(txtposisisteker.getText());
                m.setJml_kabel_LCD(Integer.parseInt(txtjmlLCD.getText()));
                m.setJml_baik_LCD(Integer.parseInt(txtjmlkondisiLCD.getText()));
                m.setPosisi_kabel_LCD(txtposisiLCD.getText());
                m.setJml_lampu(Integer.parseInt(txtjmllampu.getText()));
                m.setJml_baik_lampu(Integer.parseInt(txtjmlkondisilampu.getText()));
                m.setPosisi_lampu(txtposisilampu.getText());
                m.setJml_kipas(Integer.parseInt(txtjmlkondisikipas.getText()));
                m.setJml_baik_kipas(Integer.parseInt(txtjmlkondisikipas.getText()));
                m.setPosisi_kipas(txtposisikipas.getText());
                m.setJml_AC(Integer.parseInt(txtjmlAC.getText()));
                m.setJml_baik_AC(Integer.parseInt(txtjmlkondisiAC.getText()));
                m.setPosisi_AC(txtposisiAC.getText());
                m.setJml_CCTV(Integer.parseInt(txtjmlCCTV.getText()));
                m.setJml_baik_CCTV(Integer.parseInt(txtjmlkondisiCCTV.getText()));
                m.setPosisi_CCTV(txtposisiCCTV.getText());
                m.setSSID((String) cmbpilihSSID.getSelectedItem());
                m.setLogin(txtlogin.getText());
                m.setKondisi_lantai(txtkondisilantai.getText());
                m.setKondisi_dinding(txtkondisidinding.getText());
                m.setKondisi_atap(txtkondisiatap.getText());
                m.setKondisi_pintu(txtkondisipintu.getText());
                m.setKondisi_jendela(txtkondisijendela.getText());
                m.setKondisi_kaca_jendela(txtkondisikaca.getText());
                m.setSirkulasi_udara(txtsirkulasiudara.getText());
                m.setNilai_pencahayaan(Double.parseDouble(txtnilaipencahayaan.getText()));
                m.setKelembapan(Double.parseDouble(txtkelembapan.getText()));
                m.setSuhu(Double.parseDouble(txtsuhu.getText()));
                m.setKebisingan(txtkebisingan.getText());
                m.setBau(txtbau.getText());
                m.setKebocoran(txtkebocoran.getText());
                m.setKerusakan(txtkerusakan.getText());
                m.setKeausan(txtkeausan.getText());
                m.setKekokohan(txtkekokohan.getText());
                m.setJml_kunci_pintu(Integer.parseInt(txtjmlpintu.getText()));
                m.setJml_kunci_jendela(Integer.parseInt(txtjmljendela.getText()));
                m.setBahaya(txtbahaya.getText());
                if (konek.Insert(m) > 0) {
                    loadData();
                    currentRow = inventaris.size() - 1;
                    bindData();
                    identitas_ruang.setVisible(false);
                    data.setVisible(true);
                    tbldata.setVisible(true);
                    JOptionPane.showMessageDialog(this, "Data berhasil disimpan", "Informasi", JOptionPane.INFORMATION_MESSAGE);
                } else {
                    JOptionPane.showMessageDialog(this, "Data gagal disimpan", "Informasi", JOptionPane.INFORMATION_MESSAGE);
                }
                txtnama.setEditable(false);
                txtlokasi.setEditable(false);
                cmbfakultas.setEditable(false);
                btnnew.setText("Baru");
                konek.closeConnection(); //tutup koneksi 
            } else {
                JOptionPane.showMessageDialog(this, "Mohon isi data dengan benar", "Informasi", JOptionPane.INFORMATION_MESSAGE);
            }
        }
        tampilkandata();
    }                                      

    private void btnlihatdataActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        previous_next();
        data.setVisible(true);
        btnnext.setEnabled(false);
        btnprevious.setEnabled(false);
    }                                            

    private void btndeleteActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
            int dialog = JOptionPane.showConfirmDialog(this,
                    "Apakah Anda yakin akan menghapus data ini?",
                    "Konfirmasi", JOptionPane.YES_NO_OPTION);
            Koneksi konek = new Koneksi(); //buka koneksi 
            Model m = inventaris.get(currentRow);
            if (dialog == JOptionPane.YES_OPTION) {
                if (konek.Delete(m) > 0) {
                    loadData();
                    //currentRow = currentRow - 1;
                    bindData();
                    tampilkandata();
                    JOptionPane.showMessageDialog(this, "Data berhasil dihapus", "Informasi", JOptionPane.INFORMATION_MESSAGE);
                } else {
                    JOptionPane.showMessageDialog(this, "Data gagal dihapus", "Informasi", JOptionPane.INFORMATION_MESSAGE);
                }
            }
            konek.closeConnection(); //tutup koneksi 
    }                                         

    private void btnresetActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        txtnama.setText("");
        txtlokasi.setText("");
        cmbfakultas.setSelectedItem("Pilih Fakultas");
        txtpanjang.setText("");
        txtlebar.setText("");
        txtjmlkursi.setText("");
        txtjmlpintu.setText("");
        txtjmljendela.setText("");
        txtjmlsteker.setText("");
        txtjmlkondisisteker.setText("");
        cbsteker5.setSelected(false);
        cbsteker6.setSelected(false);
        cbsteker7.setSelected(false);
        cbsteker8.setSelected(false);
        cbsteker9.setSelected(false);
        cbsteker10.setSelected(false);
        txtposisisteker.setText("");
        txtjmlLCD.setText("");
        txtjmlkondisiLCD.setText("");
        cbLCD3.setSelected(false);
        cbLCD4.setSelected(false);
        txtposisiLCD.setText("");
        txtjmllampu.setText("");
        txtjmlkondisilampu.setText("");
        cblampu5.setSelected(false);
        cblampu6.setSelected(false);
        txtposisilampu.setText("");
        txtjmlkipas.setText("");
        txtjmlkondisikipas.setText("");
        cbkipas5.setSelected(false);
        cbkipas6.setSelected(false);
        txtposisikipas.setText("");
        txtjmlAC.setText("");
